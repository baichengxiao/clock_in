<table>
  <tr>
    <th>User Name:</th>
    <td>    <%= f.text_field :username %></td>
  </tr>
  <tr>
    <th>Password:</th>
    <td><%= f.password_field :password %></td>
  </tr>
  <tr>
    <th>Email:</th>
    <td>  <%= f.email_field :email %></td>
  </tr>
  <tr>
  <th>Role:</th>
  <td><%= f.select :role, ['manager','admin','user'] %></td>
  </tr>
</table>
